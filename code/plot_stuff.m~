clear;
for v = 0:1
    %p_toddle_slow = v/100;
    v_max = 20 + v
    x_param = v_max;
    [res_tot, road, density_of_cars] = core(x_param);
    hold on
    %plot(linspace(0,length(res_tot)-1,length(res_tot)),res_tot, '- r')
    plot(linspace(0, length(res_tot), length(res_tot))
    res_tot/length(res_tot)) %label = xparam
end
   
%ylim([0.1,0.7])
xlim([0,100])
ylabel('average flow rate')
xlabel('probability of toddling of slow cars')